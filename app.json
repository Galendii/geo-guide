{
  "expo": {
    "name": "geo-guide",
    "slug": "geo-guide",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.brenozg.geoguide",
      "versionCode": 57,
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    }
  }
}
